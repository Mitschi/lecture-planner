type Lecture {
    id: ID!
    name: String
    number: String
    lecturer: Employee
}

type Employee {
    id: ID!
    name: String
    employeeNumber: Int
}


input LectureInput {
    # id will be generated by the backend/database
    name: String
    number: String
    lecturerId: ID!
}

input EmployeeInput {
    # id will be generated by the backend/database
    name: String
    employeeNumber: Int
}